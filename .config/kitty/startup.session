# Set the layout for this tab (it will use the bias=70 defined in kitty.conf)
cd ~

# Window 1 (Left - Master)
launch zsh --no-rcs -c "fastfetch --config ~/.config/fastfetch/amazing.jsonc; source ~/.zshrc; vf; exec zsh"

# Window 2 (Right Top)
launch zsh --no-rcs -c "htop; exec zsh"

# Window 3 (Right Bottom)
launch zsh --no-rcs -c "tty-clock -c -C 4"
resize_window shorter 15

